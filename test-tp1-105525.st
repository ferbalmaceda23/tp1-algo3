TestCase subclass: #PersonaTest	instanceVariableNames: ''	classVariableNames: ''	package: 'test-tp1-105525'!!PersonaTest methodsFor: 'tests' stamp: 'FernandoBalmaceda 5/14/2021 17:49'!test02CreoUnaPersonaYNoPuedeCircularPorDefault		|persona|		persona := Persona new.		self deny: persona puedeCircular.! !!PersonaTest methodsFor: 'tests' stamp: 'FernandoBalmaceda 5/14/2021 19:00'!test04CreoUnaPersonaLeAgregoUnSintomaYSuDiagnosticoEsSospechoso	|persona|			persona := Persona new.		persona agregarSintoma: 'Fiebre'.		self assert: persona diagnostico equals: 'Sospechoso'.! !!PersonaTest methodsFor: 'tests' stamp: 'FernandoBalmaceda 5/14/2021 17:49'!test01CreoUnaPersonaConElConstructorYSuNombreEsElCorrecto 	|persona|		persona := Persona conNombre: 'Fernando'.		self assert: persona nombre equals: 'Fernando'.! !!PersonaTest methodsFor: 'tests' stamp: 'FernandoBalmaceda 5/14/2021 19:01'!test05CreoUnaPersonaLeAgrego3SintomasNoHabitualesYSuDiagnosticoEsSospechoso	|persona|		persona := Persona new.		persona agregarSintoma: 'Dolor de cabeza'.	persona agregarSintoma: 'Diarrea'.	persona agregarSintoma: 'Dolor de garganta'.		self assert: persona diagnostico equals: 'Sospechoso'.! !!PersonaTest methodsFor: 'tests' stamp: 'FernandoBalmaceda 5/14/2021 19:01'!test06CreoUnaPersonaLeAgrego3Sintomas1HabitualYSuDiagnosticoEsPositivo		|persona|		persona := Persona new.		persona agregarSintoma: 'Fiebre'.  	persona agregarSintoma: 'Dolor de cabeza'.  	persona agregarSintoma: 'Diarrea'.		self assert: persona diagnostico equals: 'Positivo'.! !!PersonaTest methodsFor: 'tests' stamp: 'FernandoBalmaceda 5/14/2021 17:50'!test03CreoUnaPersonaLaVacunoYPuedeCircular		|persona|		persona := Persona new.		persona vacunar.		self assert: persona puedeCircular.! !TestCase subclass: #SintomaTest	instanceVariableNames: ''	classVariableNames: ''	package: 'test-tp1-105525'!!SintomaTest methodsFor: 'tests' stamp: 'FernandoBalmaceda 5/14/2021 18:07'!test01CreoUnSintomaConElConstructorYSuNombreEsElIndicado	|sintoma|		sintoma := Sintoma conNombre: 'Tos'.		self assert: sintoma nombre equals: 'Tos'.! !!SintomaTest methodsFor: 'tests' stamp: 'FernandoBalmaceda 5/14/2021 18:41'!test03Creo3SintomasHabitualesYLos3EfectivamenteLoSon	|sintoma1 sintoma2 sintoma3|		sintoma1 := Sintoma conNombre: 'Tos'.	sintoma2 := Sintoma conNombre: 'Fiebre'.	sintoma3 := Sintoma conNombre: 'Cansancio'.			self assert: sintoma1 esHabitual.	self assert: sintoma2 esHabitual.	self assert: sintoma3 esHabitual.! !!SintomaTest methodsFor: 'tests' stamp: 'FernandoBalmaceda 5/14/2021 18:08'!test02CreoUnSintomaNoHabitualYEfectivamenteLoEs		|sintoma|	sintoma := Sintoma conNombre: 'Dolor de garganta'.		self deny: sintoma esHabitual.! !