Object subclass: #AlgoVid	instanceVariableNames: 'personas burbujas colegios'	classVariableNames: ''	package: 'tp1-105525'!!AlgoVid methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/13/2021 21:54'!initialize	personas := OrderedCollection new.	burbujas := OrderedCollection new.	colegios := OrderedCollection new.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 18:10'!diagnosticoDe: unNombre	^(self buscar: unNombre en: personas) diagnostico.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 20:20'!nuevaBurbuja: unNombre	burbujas add: (Burbuja conNombre: unNombre).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 14:22'!nuevaPersona: unNombre	personas add: (Persona conNombre: unNombre).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:33'!agregarA: unNombre aLaBurbuja: unaBurbuja	(self buscar: unaBurbuja en: burbujas) agregarA: (self buscar: unNombre en: personas).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:34'!agregarSintoma: unSintoma a: unNombre	(self buscar: unNombre en: personas) agregarSintoma: unSintoma.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 21:58'!nuevoColegio: unNombre	colegios add: (Colegio conNombre: unNombre).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:34'!burbujaPinchada: unaBurbuja	^(self buscar: unaBurbuja en: burbujas) estaPinchada.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:34'!clasesPresencialesEn: unColegio	^ (self buscar: unColegio en: colegios) hayClases.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:36'!puedeCircular: unNombre	^(self buscar: unNombre en: personas) puedeCircular.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:35'!esPersonaDeRiesgo: unNombre	(self buscar: unNombre en: personas) esPersonaDeRiesgo.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:34'!agregarA: unaBurbuja alColegio: unColegio	(self buscar: unColegio en: colegios) agregarBurbuja: (self buscar: unaBurbuja en: burbujas).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:35'!contactoEstrechoEntre: unNombre y: otroNombre	(self buscar: unNombre en: personas) contactoEstrechoCon: (self buscar: otroNombre en: personas).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:36'!estaVacunadx: unNombre	(self buscar: unNombre en: personas) vacunar.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:35'!esPersonalEsencial: unNombre	(self buscar: unNombre en: personas) esEsencial.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 14:55'!buscar: unNombre en: unaColeccion	^(unaColeccion detect: [ :objeto | objeto nombre = unNombre] ifNone: [nil]).! !Object subclass: #Burbuja	instanceVariableNames: 'nombre personas estaPinchada'	classVariableNames: ''	package: 'tp1-105525'!!Burbuja methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 18:33'!pinchar	|personaInfectada|	personaInfectada := personas detect: [ :persona | persona tieneCovid ].	personas do: [ :persona | (persona tieneCovid ) ifFalse: [			persona contactoEstrechoCon: personaInfectada.		]	].! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 20:16'!establecerNombre: unNombre	nombre := unNombre.! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 20:46'!agregarA: unaPersona	personas add: unaPersona.! !!Burbuja methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/15/2021 14:23'!initialize	personas := OrderedCollection new.	estaPinchada := false.! !!Burbuja methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/15/2021 18:32'!estaPinchada	(personas anySatisfy: [ :persona | persona tieneCovid ]) ifTrue: [			self pinchar.			estaPinchada := true.	].	^estaPinchada.! !!Burbuja methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/13/2021 20:16'!nombre	^nombre.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Burbuja class	instanceVariableNames: ''!!Burbuja class methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/13/2021 20:18'!conNombre: unNombre	|burbuja|.	burbuja := Burbuja new.	burbuja establecerNombre: unNombre.	^burbuja.! !Object subclass: #Colegio	instanceVariableNames: 'nombre burbujas'	classVariableNames: ''	package: 'tp1-105525'!!Colegio methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/13/2021 21:59'!initialize	burbujas := OrderedCollection new.! !!Colegio methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 21:55'!establecerNombre: unNombre	nombre := unNombre.! !!Colegio methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 22:01'!agregarBurbuja: unaBurbuja	burbujas add: unaBurbuja.! !!Colegio methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 15:15'!hayClases	^( ((burbujas count: [ :burbuja | burbuja estaPinchada ])/burbujas size) < 0.4).! !!Colegio methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/13/2021 21:54'!nombre	^nombre.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Colegio class	instanceVariableNames: ''!!Colegio class methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/13/2021 21:56'!conNombre: unNombre	|colegio|	colegio := Colegio new.	colegio establecerNombre: unNombre.	^colegio.! !Object subclass: #Diagnostico	instanceVariableNames: 'personas burbujas colegios'	classVariableNames: ''	package: 'tp1-105525'!!Diagnostico methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 17:53'!rediagnosticarSiTiene: unSintomaHabitual y: unaCantidadSintomas	self subclassResponsibility.! !!Diagnostico methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 17:56'!descripcion	self subclassResponsibility.! !!Diagnostico methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 17:54'!siguienteDiagnostico	self subclassResponsibility.! !!Diagnostico methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/15/2021 16:47'!puedeCircular	self subclassResponsibility.! !Object subclass: #Medico	instanceVariableNames: 'sintomas diagnostico'	classVariableNames: ''	package: 'tp1-105525'!!Medico methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 16:08'!pacienteTieneSintomasHabituales	^(sintomas anySatisfy: [:sintoma | sintoma esHabitual]).! !!Medico methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 18:11'!informarContactoEstrechoCon: unaPersona	(#('Positivo' 'Sospechoso') anySatisfy: [ :unDiagnostico | unDiagnostico = unaPersona diagnostico ])		ifTrue: [ diagnostico := Sospechoso new ].! !!Medico methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 18:02'!informarSintoma: unSintoma	sintomas add: (Sintoma conNombre: unSintoma).	self revisarDiagnostico.! !!Medico methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 18:10'!revisarDiagnostico	diagnostico := diagnostico rediagnosticarSiTiene: (self pacienteTieneSintomasHabituales) y: (sintomas size).! !!Medico methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 18:17'!diagnostico	^diagnostico descripcion.! !!Medico methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 18:02'!loDejaCircular	^(diagnostico puedeCircular).! !!Medico methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/15/2021 15:10'!initialize	sintomas := OrderedCollection new.	diagnostico := Negativo new.! !Diagnostico subclass: #Negativo	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105525'!!Negativo methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/15/2021 02:04'!puedeCircular	^true.! !!Negativo methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 17:54'!siguienteDiagnostico	^Sospechoso new.! !!Negativo methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 17:58'!rediagnosticarSiTiene: unSintomaHabitual y: unaCantidadSintomas	(unaCantidadSintomas > 0) ifTrue: [ ^self siguienteDiagnostico ].	^self.! !!Negativo methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 17:56'!descripcion	^'No positivo'.! !Object subclass: #Permiso	instanceVariableNames: 'esEsencial noEsPersonaDeRiesgo estaVacunadx'	classVariableNames: ''	package: 'tp1-105525'!!Permiso methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/14/2021 23:57'!esEsencial	esEsencial := true.! !!Permiso methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/15/2021 11:10'!puedeCircular	^(noEsPersonaDeRiesgo & (esEsencial | estaVacunadx)).! !!Permiso methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/15/2021 02:42'!esPersonaDeRiesgo	noEsPersonaDeRiesgo := false.! !!Permiso methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 11:08'!informarVacunacion	estaVacunadx := true.! !!Permiso methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 17:27'!tineneCovid! !!Permiso methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 15:02'!estaVacunadx	estaVacunadx := true.! !!Permiso methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/15/2021 11:18'!initialize	esEsencial := false.	noEsPersonaDeRiesgo := true.	estaVacunadx := false.! !Object subclass: #Persona	instanceVariableNames: 'nombre medico permiso'	classVariableNames: ''	package: 'tp1-105525'!!Persona methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/15/2021 11:30'!initialize	medico := Medico new.	permiso := Permiso new.! !!Persona methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/15/2021 11:12'!puedeCircular	^((permiso puedeCircular) & (medico loDejaCircular)).! !!Persona methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/13/2021 14:14'!nombre	^nombre.! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 15:05'!contactoEstrechoCon: unaPersona	medico informarContactoEstrechoCon: unaPersona.! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 15:02'!vacunar	permiso estaVacunadx.! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/14/2021 23:59'!esPersonaDeRiesgo	permiso esPersonaDeRiesgo.! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/14/2021 23:59'!esEsencial	permiso esEsencial.! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:56'!agregarSintoma: unSintoma	medico informarSintoma: unSintoma.! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 14:39'!establecerNombre: unNombre	nombre := unNombre.! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 18:16'!diagnostico	^medico diagnostico.! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 18:30'!tieneCovid	^(medico diagnostico = 'Positivo').! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Persona class	instanceVariableNames: 'nombre puedeCircular'!!Persona class methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/13/2021 14:40'!conNombre: unNombre	| persona |	persona := Persona new.	persona establecerNombre: unNombre.	^persona.! !Diagnostico subclass: #Positivo	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105525'!!Positivo methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 17:54'!siguienteDiagnostico	^self.! !!Positivo methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 17:54'!rediagnosticarSiTiene: unSintomaHabitual y: unaCantidadSintomas	^self siguienteDiagnostico.! !!Positivo methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 17:56'!descripcion	^'Positivo'! !!Positivo methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/15/2021 02:01'!puedeCircular	^false.! !Object subclass: #Sintoma	instanceVariableNames: 'nombre esHabitual'	classVariableNames: ''	package: 'tp1-105525'!!Sintoma methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/14/2021 16:07'!initialize	esHabitual := false.! !!Sintoma methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 18:44'!establecerNombre: unNombre	(#('Fiebre' 'Tos' 'Cansancio') anySatisfy: [ :unSintoma | unSintoma = unNombre ])		ifTrue: [ self establecerComoHabitual ].	nombre := unNombre.! !!Sintoma methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/14/2021 15:54'!establecerComoHabitual	esHabitual := true.! !!Sintoma methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/14/2021 15:47'!nombre	^nombre.! !!Sintoma methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/14/2021 15:48'!esHabitual	^esHabitual.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Sintoma class	instanceVariableNames: ''!!Sintoma class methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/15/2021 18:45'!conNombre: unNombre	|sintoma|	sintoma := self new.	sintoma establecerNombre: unNombre.	^sintoma.! !Diagnostico subclass: #Sospechoso	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105525'!!Sospechoso methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/14/2021 22:49'!puedeCircular	^false.! !!Sospechoso methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 17:54'!siguienteDiagnostico	^Positivo new.! !!Sospechoso methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 17:54'!rediagnosticarSiTiene: unSintomaHabitual y: unaCantidadSintomas	((unSintomaHabitual & (unaCantidadSintomas > 2)) | (unaCantidadSintomas > 3)) ifTrue: [ ^self siguienteDiagnostico ].	^self.! !!Sospechoso methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 17:56'!descripcion	^'Sospechoso'! !