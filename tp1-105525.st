TestCase subclass: #Tp1Test	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105525-test'!!Tp1Test methodsFor: 'tests' stamp: 'FernandoBalmaceda 5/13/2021 14:30'!test01SeCreaUnaPersonaYElNombreEsElCorrecto	"Arrange"	|persona|	"Act"	persona := Persona conNombre: 'Fernando'.	"Assert"	self assert: (persona nombre) equals: 'Fernando'.! !Object subclass: #AlgoVid	instanceVariableNames: 'personas burbujas colegios'	classVariableNames: ''	package: 'tp1-105525'!!AlgoVid methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/13/2021 21:54'!initialize	personas := OrderedCollection new.	burbujas := OrderedCollection new.	colegios := OrderedCollection new.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 14:49'!diagnosticoDe: unNombre	^(personas detect: [ :persona | persona nombre = unNombre]) diagnostico.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 20:20'!nuevaBurbuja: unNombre	burbujas add: (Burbuja conNombre: unNombre).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 14:22'!nuevaPersona: unNombre	personas add: (Persona conNombre: unNombre).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 21:20'!agregarA: unNombre aLaBurbuja: unaBurbuja	|unaPersona|.	unaPersona := personas detect: [ :persona | persona nombre = unNombre ].	(burbujas detect: [ :burbuja | burbuja nombre = unaBurbuja ]) agregarA: unaPersona.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 14:44'!agregarSintoma: unSintoma a: unNombre	(personas detect: [ :persona | persona nombre = unNombre]) agregarSintoma: unSintoma.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 21:58'!nuevoColegio: unNombre	colegios add: (Colegio conNombre: unNombre).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 21:32'!burbujaPinchada: unaBurbuja	^(burbujas detect: [ :burbuja | burbuja nombre = unaBurbuja]) estaPinchada.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 22:49'!clasesPresencialesEn: unColegio	^ (colegios detect: [ :colegio | colegio nombre = unColegio]) hayClases.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 20:55'!puedeCircular: unNombre	^(personas detect: [ :persona | persona nombre = unNombre ]) puedeCircular.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 20:06'!esPersonaDeRiesgo: unNombre	(personas detect: [ :persona | persona nombre = unNombre]) esPersonaDeRiesgo.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 22:06'!agregarA: unaBurbuja alColegio: unColegio	|colegioBuscado|	colegioBuscado := (colegios detect: [ :colegio | colegio nombre = unColegio ]).	colegioBuscado agregarBurbuja: (burbujas detect: [ :burbuja | burbuja nombre = unaBurbuja ])! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 20:04'!contactoEstrechoEntre: unNombre y: otroNombre	| unaPersona |.	unaPersona := personas detect: [ :persona | persona nombre = otroNombre ].	(personas detect: [ :persona | persona nombre = unNombre]) contactoEstrechoCon: unaPersona.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 14:42'!estaVacunadx: unNombre	(personas detect: [ :persona | persona nombre = unNombre ]) vacunar.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 16:19'!esPersonalEsencial: unNombre	(personas detect: [ :persona | persona nombre = unNombre ]) esEsencial.! !Object subclass: #Burbuja	instanceVariableNames: 'nombre personas estaPinchada'	classVariableNames: ''	package: 'tp1-105525'!!Burbuja methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 20:16'!establecerNombre: unNombre	nombre := unNombre.! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 20:46'!agregarA: unaPersona	personas add: unaPersona.! !!Burbuja methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/13/2021 20:34'!initialize	personas := OrderedCollection new.	estaPinchada := false.! !!Burbuja methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/13/2021 21:50'!estaPinchada	|unaPersona|	unaPersona := personas detect: [ :persona | persona diagnostico = 'Positivo' ] ifNone: [ nil ].	unaPersona ifNotNil: [		personas do: [ :persona | (persona = unaPersona nombre) ifFalse: [ 				persona contactoEstrechoCon: unaPersona.			].		].		estaPinchada := true	].	^estaPinchada.! !!Burbuja methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/13/2021 20:16'!nombre	^nombre.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Burbuja class	instanceVariableNames: ''!!Burbuja class methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/13/2021 20:18'!conNombre: unNombre	|burbuja|.	burbuja := Burbuja new.	burbuja establecerNombre: unNombre.	^burbuja.! !Object subclass: #Colegio	instanceVariableNames: 'nombre burbujas'	classVariableNames: ''	package: 'tp1-105525'!!Colegio methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/13/2021 21:59'!initialize	burbujas := OrderedCollection new.! !!Colegio methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 21:55'!establecerNombre: unNombre	nombre := unNombre.! !!Colegio methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 22:01'!agregarBurbuja: unaBurbuja	burbujas add: unaBurbuja.! !!Colegio methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 22:59'!hayClases	|burbujasPinchadas|.	burbujasPinchadas := burbujas count: [ :burbuja |	 burbuja estaPinchada ].	^(((burbujasPinchadas/burbujas size)*100) < 40 ).! !!Colegio methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/13/2021 21:54'!nombre	^nombre.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Colegio class	instanceVariableNames: ''!!Colegio class methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/13/2021 21:56'!conNombre: unNombre	|colegio|	colegio := Colegio new.	colegio establecerNombre: unNombre.	^colegio.! !Object subclass: #Persona	instanceVariableNames: 'nombre puedeCircular sintomas diagnostico personaDeRiesgo'	classVariableNames: ''	package: 'tp1-105525'!!Persona methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/13/2021 20:08'!initialize	puedeCircular := false.	diagnostico := 'Sospechoso'.	sintomas := OrderedCollection new.	personaDeRiesgo := false.! !!Persona methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/13/2021 20:09'!puedeCircular	(sintomas isEmpty) ifFalse: [ puedeCircular := false ].	(personaDeRiesgo) ifTrue: [ puedeCircular := false ].	^puedeCircular.! !!Persona methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/13/2021 14:14'!nombre	^nombre.! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 21:06'!contactoEstrechoCon: unaPersona	( (unaPersona diagnostico = 'Sospechoso') | (unaPersona diagnostico = 'Positivo') ) ifTrue: [ 		puedeCircular := false.	].! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 14:42'!vacunar	puedeCircular := true.! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 20:08'!esPersonaDeRiesgo	personaDeRiesgo := true.! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 16:16'!esEsencial	puedeCircular := true.! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 14:45'!agregarSintoma: unSintoma	sintomas add: unSintoma.! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 14:39'!establecerNombre: unNombre	nombre := unNombre.! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 16:13'!diagnostico	(sintomas isEmpty) ifFalse: [ diagnostico := 'Sospechoso' ].	(sintomas size = 3) ifTrue: [ 		(sintomas detect: [ :sintoma | sintoma = 'Fiebre' ]) ifNotNil: [ diagnostico := 'Positivo' ]	].	(sintomas size > 3) ifTrue: [ diagnostico := 'Positivo' ].	^diagnostico.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Persona class	instanceVariableNames: 'nombre puedeCircular'!!Persona class methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/13/2021 14:40'!conNombre: unNombre	| persona |	persona := Persona new.	persona establecerNombre: unNombre.	^persona.! !