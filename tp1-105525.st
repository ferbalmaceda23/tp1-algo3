Object subclass: #AlgoVid	instanceVariableNames: 'personas burbujas colegios'	classVariableNames: ''	package: 'tp1-105525'!!AlgoVid methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/13/2021 21:54'!initialize	personas := OrderedCollection new.	burbujas := OrderedCollection new.	colegios := OrderedCollection new.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:35'!diagnosticoDe: unNombre	^(self buscar: unNombre en: personas) diagnostico.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 20:20'!nuevaBurbuja: unNombre	burbujas add: (Burbuja conNombre: unNombre).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 14:22'!nuevaPersona: unNombre	personas add: (Persona conNombre: unNombre).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:33'!agregarA: unNombre aLaBurbuja: unaBurbuja	(self buscar: unaBurbuja en: burbujas) agregarA: (self buscar: unNombre en: personas).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:34'!agregarSintoma: unSintoma a: unNombre	(self buscar: unNombre en: personas) agregarSintoma: unSintoma.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 21:58'!nuevoColegio: unNombre	colegios add: (Colegio conNombre: unNombre).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:34'!burbujaPinchada: unaBurbuja	^(self buscar: unaBurbuja en: burbujas) estaPinchada.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:34'!clasesPresencialesEn: unColegio	^ (self buscar: unColegio en: colegios) hayClases.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:36'!puedeCircular: unNombre	^(self buscar: unNombre en: personas) puedeCircular.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:35'!esPersonaDeRiesgo: unNombre	(self buscar: unNombre en: personas) esPersonaDeRiesgo.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:34'!agregarA: unaBurbuja alColegio: unColegio	(self buscar: unColegio en: colegios) agregarBurbuja: (self buscar: unaBurbuja en: burbujas).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:35'!contactoEstrechoEntre: unNombre y: otroNombre	(self buscar: unNombre en: personas) contactoEstrechoCon: (self buscar: otroNombre en: personas).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:36'!estaVacunadx: unNombre	(self buscar: unNombre en: personas) vacunar.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:35'!esPersonalEsencial: unNombre	(self buscar: unNombre en: personas) esEsencial.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:33'!buscar: unNombre en: unaColeccion	^(unaColeccion detect: [ :objeto | objeto nombre = unNombre]).! !Object subclass: #Burbuja	instanceVariableNames: 'nombre personas estaPinchada'	classVariableNames: ''	package: 'tp1-105525'!!Burbuja methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 20:16'!establecerNombre: unNombre	nombre := unNombre.! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 20:46'!agregarA: unaPersona	personas add: unaPersona.! !!Burbuja methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/13/2021 20:34'!initialize	personas := OrderedCollection new.	estaPinchada := false.! !!Burbuja methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/13/2021 21:50'!estaPinchada	|unaPersona|	unaPersona := personas detect: [ :persona | persona diagnostico = 'Positivo' ] ifNone: [ nil ].	unaPersona ifNotNil: [		personas do: [ :persona | (persona = unaPersona nombre) ifFalse: [ 				persona contactoEstrechoCon: unaPersona.			].		].		estaPinchada := true	].	^estaPinchada.! !!Burbuja methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/13/2021 20:16'!nombre	^nombre.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Burbuja class	instanceVariableNames: ''!!Burbuja class methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/13/2021 20:18'!conNombre: unNombre	|burbuja|.	burbuja := Burbuja new.	burbuja establecerNombre: unNombre.	^burbuja.! !Object subclass: #Colegio	instanceVariableNames: 'nombre burbujas'	classVariableNames: ''	package: 'tp1-105525'!!Colegio methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/13/2021 21:59'!initialize	burbujas := OrderedCollection new.! !!Colegio methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 21:55'!establecerNombre: unNombre	nombre := unNombre.! !!Colegio methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 22:01'!agregarBurbuja: unaBurbuja	burbujas add: unaBurbuja.! !!Colegio methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/14/2021 12:10'!hayClases	^( (((burbujas count: [ :burbuja | burbuja estaPinchada ])/burbujas size)*100) < 40).! !!Colegio methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/13/2021 21:54'!nombre	^nombre.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Colegio class	instanceVariableNames: ''!!Colegio class methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/13/2021 21:56'!conNombre: unNombre	|colegio|	colegio := Colegio new.	colegio establecerNombre: unNombre.	^colegio.! !Object subclass: #Medico	instanceVariableNames: 'sintomas diagnostico'	classVariableNames: ''	package: 'tp1-105525'!!Medico methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 02:29'!elPaciente: unaPersona tuvoContactoEstrechoCon: otraPersona	(#('Positivo' 'Sospechoso') anySatisfy: [ :sintoma | sintoma = otraPersona diagnostico ])		ifTrue: [ diagnostico := Sospechoso new. ].! !!Medico methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:45'!informarSintoma: unSintoma	sintomas add: (Sintoma conNombre: unSintoma).! !!Medico methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:47'!diagnosticar	(sintomas isEmpty) ifFalse: [ diagnostico := Sospechoso new ].	(sintomas size = 3) ifTrue: [		(sintomas anySatisfy: [ :sintoma | sintoma esHabitual ]) ifTrue: [ diagnostico := Positivo new ].	].	(sintomas size > 3) ifTrue: [ diagnostico := Positivo new ].	^diagnostico.! !!Medico methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:59'!loDejaCircular	^(#('Positivo' 'Sospechoso') anySatisfy: [ :e | (self diagnosticar) diagnostico = e ]) not.	! !!Medico methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/15/2021 02:25'!initialize	sintomas := OrderedCollection new.	diagnostico := NoPositivo new.! !Object subclass: #NoPositivo	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105525'!!NoPositivo methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 02:03'!diagnostico	^'No positivo'.! !!NoPositivo methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/15/2021 02:04'!puedeCircular	^true.! !Object subclass: #PermisoCirculacion	instanceVariableNames: 'puedeCircular esEsencial noEsPersonaDeRiesgo'	classVariableNames: ''	package: 'tp1-105525'!!PermisoCirculacion methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/15/2021 02:41'!initialize	puedeCircular := false.	esEsencial := false.	noEsPersonaDeRiesgo := true.! !!PermisoCirculacion methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/14/2021 23:57'!esEsencial	esEsencial := true.! !!PermisoCirculacion methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/15/2021 02:44'!puedeCircular	^(noEsPersonaDeRiesgo & esEsencial).! !!PermisoCirculacion methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/15/2021 02:42'!esPersonaDeRiesgo	noEsPersonaDeRiesgo := false.! !Object subclass: #Persona	instanceVariableNames: 'nombre medico permiso vacunadx'	classVariableNames: ''	package: 'tp1-105525'!!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 02:38'!contactoEstrechoCon: unaPersona	medico elPaciente: self tuvoContactoEstrechoCon: unaPersona.! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 00:46'!vacunar	vacunadx := true.! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/14/2021 23:59'!esPersonaDeRiesgo	permiso esPersonaDeRiesgo.! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/14/2021 23:59'!esEsencial	permiso esEsencial.! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:56'!agregarSintoma: unSintoma	medico informarSintoma: unSintoma.! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/13/2021 14:39'!establecerNombre: unNombre	nombre := unNombre.! !!Persona methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/15/2021 01:52'!diagnostico	^(medico diagnosticar) diagnostico.! !!Persona methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/15/2021 03:07'!puedeCircular	^((permiso puedeCircular | vacunadx) & (medico loDejaCircular)).! !!Persona methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/13/2021 14:14'!nombre	^nombre.! !!Persona methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/15/2021 01:56'!initialize	medico := Medico new.	permiso := PermisoCirculacion new.	vacunadx := false.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Persona class	instanceVariableNames: 'nombre puedeCircular'!!Persona class methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/13/2021 14:40'!conNombre: unNombre	| persona |	persona := Persona new.	persona establecerNombre: unNombre.	^persona.! !Object subclass: #Positivo	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105525'!!Positivo methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/14/2021 22:20'!diagnostico	^'Positivo'! !!Positivo methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/15/2021 02:01'!puedeCircular	^false.! !Object subclass: #Sintoma	instanceVariableNames: 'nombre esHabitual'	classVariableNames: ''	package: 'tp1-105525'!!Sintoma methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/14/2021 16:07'!initialize	esHabitual := false.! !!Sintoma methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/14/2021 15:47'!establecerNombre: unNombre	nombre := unNombre.! !!Sintoma methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/14/2021 15:54'!establecerComoHabitual	esHabitual := true.! !!Sintoma methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/14/2021 15:47'!nombre	^nombre.! !!Sintoma methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/14/2021 15:48'!esHabitual	^esHabitual.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Sintoma class	instanceVariableNames: ''!!Sintoma class methodsFor: 'initialization' stamp: 'FernandoBalmaceda 5/14/2021 18:22'!conNombre: unNombre	|sintoma|	sintoma := Sintoma new.	sintoma establecerNombre: unNombre.	(#('Fiebre' 'Tos' 'Cansancio') anySatisfy: [ :unSintoma | unSintoma = unNombre ])		ifTrue: [ sintoma establecerComoHabitual ].	^sintoma.! !Object subclass: #Sospechoso	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105525'!!Sospechoso methodsFor: 'as yet unclassified' stamp: 'FernandoBalmaceda 5/14/2021 22:20'!diagnostico	^'Sospechoso'! !!Sospechoso methodsFor: 'accessing' stamp: 'FernandoBalmaceda 5/14/2021 22:49'!puedeCircular	^false.! !